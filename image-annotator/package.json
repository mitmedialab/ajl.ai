{
  "name": "image-annotator",
  "description": "An application to manually tag images to support diversity in facial recognition image corpora",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server",
    "lint": "eslint --ext js,jsx src",
    "test": "node jest/run.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/fileStub.js",
      "^.+\\.(scss|css)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(dist|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.9.0",
    "find-cache-dir": "^0.1.1",
    "hard-source-webpack-plugin": "^0.3.8",
    "html-webpack-plugin": "^2.26.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.0",
    "node-object-hash": "^1.1.5",
    "postcss-loader": "^1.2.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
